<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <!-- Warna default tombol -->
        <shape android:shape="rectangle">
            <solid android:color="#DCDCDC" /> <!-- Warna latar belakang tombol -->
            <stroke android:color="#67000000" android:width="1dp" /> <!-- Border atau garis tepi -->
            <corners android:radius="7dp" /> <!-- Sudut melengkung -->
        </shape>
    </item>

</selector>
